@using ParcelInput.Models;

<h1>Here is a list of the parcels you've entered:</h1>
@if (Model.Count == 0)
{
  <p>There are no parcels in your list.</p>
}

<ul>
    @foreach(Parcel parcel in Model)
    {
        <li>Volume: @parcel.Volume()</li>
        <li>Price: @parcel.CostToShip()</li>

    }
</ul>